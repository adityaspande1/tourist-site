<script>
	
	$(document).ready(function() {
		toastr.options = {
			"positionClass": "toast-bottom-left",
		}
		
		var notificationString = '[{"notificationURL":"https://docs.google.com/forms/d/e/1FAIpQLSeeSj6jP9mAba8zkRt8gPvGP_92TZp3_to_s5LbTxwbZ_GRFg/viewform?usp=sf_link","notificationTitle":"Register for free tour of Dr. Babasaheb Ambedkar Mumbai circuit","isNewNotification":"0"},{"notificationURL":"https://www.maharashtratourism.gov.in/documents/38210/337951/Safety+Measures+for+Monsoon+by+Maharashtra+Tourism.pdf","notificationTitle":"Monsoon safety Guidelines","isNewNotification":"0"},{"notificationURL":"https://www.maharashtratourism.gov.in/dubai-expo","notificationTitle":"Glimpses of Maharashtra at Dubai Expo 2021","isNewNotification":"0"}]';
		
		if(notificationString != "") {
			var notificationArray = JSON.parse(notificationString);
			
			console.log("notificationArray ::::: ", notificationArray);
			
			var notificationCount = 0;
			for(var i=0;i<notificationArray.length;i++) {
				var jsonObject = notificationArray[i];
				var htmlContent = '<li class="lang-list"><a href="'+ jsonObject["notificationURL"] +'" target="_blank" class="line-clamp-2">'+ jsonObject["notificationTitle"] +'</a></li>';
				$(".notification-links").prepend(htmlContent);
				
				if(parseInt(jsonObject["isNewNotification"])) {
					notificationCount++;
				}
			}
			
			$("#mdot-notification-count-id").html(notificationCount);
			
			$(".mega-dropdown3").hover(
				function() { 
					$('.mega-menu3', this).fadeIn("fast");
				},
				function() {
					$('.mega-menu3', this).fadeOut("fast");
			});	
		}
		
	});
	
	function uploadNotificationContent() {
		var title = $("#uploaded-content-notification-title-id").val();
		if(title.length <= 0) {
			toastr.warning("Empty","Title Cannot be Empty.");
			return;
		}
		if(title.length > 100) {
			toastr.warning("Maxmimum Characters Breached","Title Cannot have more than 100 Characters.");
			return
		}
		
		var uploadType = $("#type-of-document").val();
		if(uploadType == "") {
			toastr.warning("Selection Empty","Please Select Type");
			return;
		}
		else if(uploadType == "document") {
			var fileExtension = $("#uploaded-content-document-id").val().split(".").pop();
			if(!(fileExtension == "pdf" || fileExtension == "doc" || fileExtension == "docx" || fileExtension == "xls" || fileExtension == "xlsx")) {
				toastr.warning("Not Allowed","The Only allowed Formats are: pdf, doc, docx, xls and xlsx.");
				return;
			}
			
			var fileSize = $("#uploaded-content-document-id").get(0).files[0].size;
			if(fileSize > 10485760){
				toastr.warning("Not Allowed","Maximum allowed size is 10MB.");
